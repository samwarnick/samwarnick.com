    "scripts": {
        "build": "npm run build:functions && ELEVENTY_ENV=production NODE_ENV=production tailwindcss -i ./src/styles.css -o ./_tmp/styles.css && node hack.js && eleventy",
        "build:functions": "esbuild netlify/functions/*.js --bundle --outdir=lambda --platform=node",
        "watch": "eleventy --watch",
        "serve": "npm run build:functions && ELEVENTY_ENV=development eleventy --serve & tailwindcss -i ./src/styles.css -o ./_tmp/styles.css --watch",
        "start": "eleventy --serve",
        "debug": "DEBUG=* eleventy"
    },
